{
  "env": {
    "webpack": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "css-modules-transform"
      ],
      "presets": [
       "env", {
        "targets": {
              "browsers": [
                "last 2 versions",
                "not ie <= 11"
              ]
            },
        "loose": true,
        "modules": false,
        "useBuiltIns": true
      }
      ]
    },
    "e2e": {
      "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "css-modules-transform"
      ],
      "presets": [
        "env", {
        "targets": {
              "browsers": [
                "last 2 versions",
                "not ie <= 11"
              ]
            },
        "loose": true,
        "modules": false,
        "useBuiltIns": true
      }
      ]
    },
    "node": {
      "presets": [
        "env", {
        "targets": {
              "browsers": [
                "last 2 versions",
                "not ie <= 11"
              ]
            },
        "loose": true,
        "modules": false,
        "useBuiltIns": true
      }
      ]
    }
  }
}
